<Application x:Class="HM.Presentation.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:views="clr-namespace:HM.Presentation.WPF.Views"
             xmlns:viewModels="clr-namespace:HM.Presentation.WPF.ViewModels">
    <Application.Resources>
        <DataTemplate DataType="{x:Type viewModels:BookingViewModel}">
            <views:BookingsView />
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewModels:RoomViewModel}">
            <views:RoomView />
        </DataTemplate>
    </Application.Resources>
</Application>
