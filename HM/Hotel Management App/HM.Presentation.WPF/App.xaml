<Application x:Class="HM.Presentation.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:views="clr-namespace:HM.Presentation.WPF.Views"
             xmlns:viewModels="clr-namespace:HM.Presentation.WPF.ViewModels"
             xmlns:rooms="clr-namespace:HM.Presentation.WPF.ViewModels.Rooms"
             xmlns:rooms1="clr-namespace:HM.Presentation.WPF.Views.Rooms"
             xmlns:bookings="clr-namespace:HM.Presentation.WPF.ViewModels.Bookings"
             xmlns:bookings1="clr-namespace:HM.Presentation.WPF.Views.Bookings">
    <Application.Resources>
        <DataTemplate DataType="{x:Type bookings:BookingViewModel}">
            <bookings1:BookingsView />
        </DataTemplate>
        <DataTemplate DataType="{x:Type rooms:RoomViewModel}">
            <rooms1:RoomView />
        </DataTemplate>
    </Application.Resources>
</Application>